{
    "name": "root/html",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Root\\Html\\": "src/",
            "FormMail\\": "lib/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpmd/phpmd" : "@stable",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3"
    },
    "scripts":{
        "phpcs": "./vendor/bin/phpcs --standard=phpcs.xml",
        "phpmd": "./vendor/bin/phpmd ./app text phpmd.xml --suffixes php --exclude vendor",
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon",
        "phpunit": "./vendor/bin/phpunit"
    }
}
